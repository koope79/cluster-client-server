#JSGF V1.0;

grammar cmdGrammar;

public <basicCmd> = <start> <cmdTimer>;
<cmdTimer> = <actionTimer> <timer> <pretext> <parameterTimer> <time>;
<timer> = (таймер);
<actionTimer> = (включи|поставь);
<parameterTimer> = (одну|пять|десять);
<time> = (минут|минуту);
<pretext> = (на);
<start> = [умный дом | дом];
